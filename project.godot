; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Shadow Shrine"
config/version="0.0.1.3"
run/main_scene="res://scenes/levels/_lev_launcher.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=120
boot_splash/show_image=false
boot_splash/image="res://assets/meta/boot_splash_a.png"
config/icon="res://assets/meta/game_icon_a.png"
config/windows_native_icon="res://assets/meta/windows/game_icon_a_128.ico"
config/data_index="res://resources/data/index.tres"

[audio]

buses/default_bus_layout="res://resources/audio_bus_layout.tres"

[autoload]

tools="*res://scripts/core_tools/tools.gd"
app="*res://scripts/core/app_manager.gd"
world="*res://scripts/core/world_manager.gd"
characters="*res://scripts/core/characters_manager.gd"
camera_manager="*res://scripts/core/camera_manager.gd"
game="*res://scripts/core/game_manager.gd"
widgets="*res://scripts/core/widget_manager.gd"
dev="*res://_dev/_dev_global.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"
window/vsync/vsync_mode=2

[editor]

naming/node_name_num_separator=2

[editor_plugins]

enabled=PackedStringArray("res://addons/gpu_trail/plugin.cfg")

